{
    "author": {
        "email": "admin@ovfteam.com",
        "name": "ovftank",
        "url": "https://ovfteam.com"
    },
    "build": {
        "appId": "com.ovfteam.mail-merger-sender",
        "compression": "maximum",
        "nsis": {
            "createDesktopShortcut": true,
            "deleteAppDataOnUninstall": true,
            "installerIcon": "assets/icon.ico",
            "installerLanguages": [
                "vi_VN"
            ],
            "oneClick": true
        },
        "productName": "Mail Merger Sender",
        "win": {
            "icon": "assets/icon.ico",
            "target": [
                {
                    "arch": [
                        "x64"
                    ],
                    "target": "nsis"
                }
            ]
        }
    },
    "dependencies": {
        "axios": "1.11.0",
        "playwright": "1.54.2"
    },
    "description": "ovfteam - cung cấp dịch vụ phát triển website trọn gói, tool MMO chuyên nghiệp và vps giá rẻ.",
    "devDependencies": {
        "electron": "37.3.0",
        "electron-builder": "26.0.12",
        "prettier": "3.6.2",
        "prettier-plugin-tailwindcss": "0.6.14"
    },
    "imports": {
        "#modules/*": "./modules/*"
    },
    "main": "index.js",
    "name": "mail-merger-sender",
    "packageManager": "pnpm@10.14.0",
    "productName": "Mail Merger Sender",
    "scripts": {
        "build": "electron-builder --win",
        "format": "prettier --write .",
        "start": "electron ."
    },
    "type": "module",
    "version": "1.0.0"
}
